<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AyteeDE.SceneSwitcher.MAUIApp.Pages.ApplicationSwitcherPage"
             Title="Application Switching">
    <ScrollView>
        <VerticalStackLayout
            HorizontalOptions="Center"
            VerticalOptions="Center"
            Spacing="75">
            <Button
                WidthRequest="300"
                FontSize="Title"
                x:Name="cmdToggle"
                Text="Start"
                Clicked="Toggle_Clicked"/>
            <Label
                HeightRequest="300"
                x:Name="lblLog" />
        </VerticalStackLayout>
    </ScrollView>
</ContentPage>
